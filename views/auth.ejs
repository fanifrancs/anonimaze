<%- include ('./partials/header') %>
	<% if (!currentUser) { %>
		<script>
			location.replace('/login');
		</script>
	<% } else { %>
		<script>
			setTimeout(() => {
				location.replace('/<%= currentUser.username %>/messages');
			}, 1000);
		</script>
	<% } %>
<%- include ('./partials/footer') %>
